{
  "name": "template-literal-tags",
  "version": "0.1.0",
  "description": "tags for template literals",
  "repository": "parro-it/template-literal-tags",
  "author": "Andrea Parodi <andrea@parro.it>",
  "license": "MIT",
  "files": [
    "index.js",
    "example.js"
  ],
  "scripts": {
    "test": "ava && linterjs .",
    "dev": "ava --watch & linterjs . --watch",
    "start": "node -r @std/esm ./index.js",
    "doc": "documentation readme index.js --section=API"
  },
  "engines": {
    "node": ">8.8"
  },
