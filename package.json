      "@std/esm"
    ]
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  },
  "devDependencies": {
    "@std/esm": "^0.12.4",
    "ava": "^0.23.0",
    "documentation": "^5.3.3",
    "linterjs": "^1.8.0"
  },
  "dependencies": {
    "compose-function": "^3.0.3",
    "justified": "^0.2.1",
    "reverse-arguments": "^1.0.0",
    "unthis": "^1.1.0"
  }
